@model System.Web.Mvc.HandleErrorInfo
<!-- Main content -->
<section class="content mt-2">
    <div class="card">
        <div class="card-header">
            <h1 class="text-warning text-center">Error Page</h1>
        </div>
        <div class="card-body text-center">
            <h3><i class="fas fa-exclamation-triangle text-danger"></i> Oops! Something went wrong. Please contact with administrator </h3>
            <p>
                We will work on fixing that right away.
                Meanwhile, you may <a href="@Url.Action("Index","Dashboard")">return to dashboard</a> or try using the search form.
            </p>
            <small class="text-danger">@Model.Exception.Message</small>
        </div>
    </div>
    <!-- /.error-page -->

</section>
<!-- /.content -->
